{
  "NLog": {
    "targets": {
      "console": {
        "type": "Console"
      },

      "controlLogging": {
        "type": "ControlLogging",
        "Host": "localhost"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "console"
      },
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "controlLogging"
      }
    ]
  },

  "Server": {
    //"URL": "https://api.foxtaur.me"
    "URL": "http://localhost:5035"
  }
}